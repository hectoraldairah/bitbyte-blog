<header role="banner" class="site-head">
  <div class="wrapper">
    <div class="site-head__inner">
      <div class="site-head__inner__themes">
        <ul id="themes">
          {% for item in themes.item %}
          <li><span id="{{ item.name }}" class="[ circle {{ item.color }} ]"></span></li>
          {% endfor %}
        </ul>
      </div>
      <nav class="[ nav ] [ site-head__nav ]" aria-label="Primary">
        <ul class="nav__list">
          {% for item in collections.all | eleventyNavigation %}
          <li>
            <a href="{{item.url}}" {{ helpers.getLinkActiveState(item.url, page.url) | safe }}>{{item.title}}</a>
          </li>
          {% endfor %}
        </ul>
      </nav>
    </div>
  </div>
</header>
