<div id="statuscafe" class="[ status-cafe pixelBorder--2 ]">
  <div class="[ status-cafe__username ]">
    <p>
      <a href="https://status.cafe/users/_bitbyte_" target="_blank"
        >{{status.author}}</a
      >
      <span>{{status.face}}</span>
    </p>
    <p>{{status.timeAgo}}</p>
  </div>
  <div id="statuscafe-content">
    <p>{{status.content}}</p>
  </div>
</div>
